{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "arrow-body-style": 0,
        "camelcase": 0,
        "react/prop-types": 0,
        "react/no-array-index-key": 0,
        "consistent-return": 0,
        ""
        "react/jsx-props-no-spreading": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    }
}